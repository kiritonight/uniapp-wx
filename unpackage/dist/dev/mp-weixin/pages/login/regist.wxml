<view class="content"><view class="logodiv"><view class="logo"><image class="imglogo" src="../../static/logo.png" mode="widthFix"></image></view></view><view class="form"><view class="username"><input class="nameform" type="text" placeholder="请输入用户名" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/></view><view class="name"><input class="name" type="text" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['','realname','$event',[]]]]]]}}" value="{{realname}}" bindinput="__e"/></view><view class="password"><input class="pawform" type="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="passwordup"><input class="pawformup" type="password" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','checkword','$event',[]]]]]]}}" value="{{checkword}}" bindinput="__e"/></view><view class="registBtn" hover-class="registBtnhover" data-event-opts="{{[['tap',[['loginrequest',['$event']]]]]}}" bindtap="__e"><text class="btnvalue">注册</text></view><view class="loginBtn"><navigator class="login" url="./login" open-type="redirect">返回登陆</navigator></view></view></view>