
.content {
	background: #3F536E;
	width: 100vw;
	height: 100vh;
}
.logodiv{
	width: 100vw;
	height: 30vh;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align:end;
	-webkit-align-items:flex-end;
	        align-items:flex-end;
}
.logo{
	width: 200rpx;
	height: 200rpx;
	overflow: hidden;
}
.logo .imglogo{
	width: 100%;
}
.form{
	padding: 0 100rpx;
	margin-top: 80rpx;
}
.loginBtn{
	width: 100%;
	height: 60rpx;
	background: #007cb1;
	border-radius: 50rpx;
	margin-top: 50rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.loginBtn .btnvalue{
	color: white;
}

